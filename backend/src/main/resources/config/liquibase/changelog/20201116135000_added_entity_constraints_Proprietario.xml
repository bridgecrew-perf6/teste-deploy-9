<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Proprietario.
    -->
    <changeSet id="20201116135000-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="proprietario"
                                 constraintName="fk_proprietario_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="dados_basicos_id"
                                 baseTableName="proprietario"
                                 constraintName="fk_proprietario_dados_basicos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dados_basicos"/>

    </changeSet>
</databaseChangeLog>
