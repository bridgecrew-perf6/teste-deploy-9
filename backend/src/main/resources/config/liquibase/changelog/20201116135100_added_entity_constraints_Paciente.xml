<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Paciente.
    -->
    <changeSet id="20201116135100-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="endereco_id"
                                 baseTableName="paciente"
                                 constraintName="fk_paciente_endereco_id"
                                 referencedColumnNames="id"
                                 referencedTableName="endereco"/>

        <addForeignKeyConstraint baseColumnNames="contato_id"
                                 baseTableName="paciente"
                                 constraintName="fk_paciente_contato_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contato"/>

        <addForeignKeyConstraint baseColumnNames="dados_basicos_id"
                                 baseTableName="paciente"
                                 constraintName="fk_paciente_dados_basicos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dados_basicos"/>

        <addForeignKeyConstraint baseColumnNames="clinica_id"
                                 baseTableName="paciente"
                                 constraintName="fk_paciente_clinica_id"
                                 referencedColumnNames="id"
                                 referencedTableName="clinica"/>

    </changeSet>
</databaseChangeLog>
