<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Consulta.
    -->
    <changeSet id="20201116135700-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="procedimento_id"
                                 baseTableName="consulta"
                                 constraintName="fk_consulta_procedimento_id"
                                 referencedColumnNames="id"
                                 referencedTableName="procedimento"/>

        <addForeignKeyConstraint baseColumnNames="clinica_id"
                                 baseTableName="consulta"
                                 constraintName="fk_consulta_clinica_id"
                                 referencedColumnNames="id"
                                 referencedTableName="clinica"/>

    </changeSet>
</databaseChangeLog>
