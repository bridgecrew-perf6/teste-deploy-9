<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Clinica.
    -->
    <changeSet id="20201116134600-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="saldo_id"
                                 baseTableName="clinica"
                                 constraintName="fk_clinica_saldo_id"
                                 referencedColumnNames="id"
                                 referencedTableName="saldo"/>

        <addForeignKeyConstraint baseColumnNames="endereco_id"
                                 baseTableName="clinica"
                                 constraintName="fk_clinica_endereco_id"
                                 referencedColumnNames="id"
                                 referencedTableName="endereco"/>

        <addForeignKeyConstraint baseColumnNames="contato_id"
                                 baseTableName="clinica"
                                 constraintName="fk_clinica_contato_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contato"/>

        <addForeignKeyConstraint baseColumnNames="clinica_id"
                                 baseTableName="clinica_proprietario"
                                 constraintName="fk_clinica_proprietario_clinica_id"
                                 referencedColumnNames="id"
                                 referencedTableName="clinica"/>

        <addForeignKeyConstraint baseColumnNames="proprietario_id"
                                 baseTableName="clinica_proprietario"
                                 constraintName="fk_clinica_proprietario_proprietario_id"
                                 referencedColumnNames="id"
                                 referencedTableName="proprietario"/>
        
    </changeSet>
</databaseChangeLog>
