<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Profissional.
    -->
    <changeSet id="20201116135200-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="profissional"
                                 constraintName="fk_profissional_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="endereco_id"
                                 baseTableName="profissional"
                                 constraintName="fk_profissional_endereco_id"
                                 referencedColumnNames="id"
                                 referencedTableName="endereco"/>

        <addForeignKeyConstraint baseColumnNames="contato_id"
                                 baseTableName="profissional"
                                 constraintName="fk_profissional_contato_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contato"/>

        <addForeignKeyConstraint baseColumnNames="tipo_profissional_id"
                                 baseTableName="profissional"
                                 constraintName="fk_profissional_tipo_profissional_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tipo_profissional"/>

        <addForeignKeyConstraint baseColumnNames="dados_basicos_id"
                                 baseTableName="profissional"
                                 constraintName="fk_profissional_dados_basicos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dados_basicos"/>

        <addForeignKeyConstraint baseColumnNames="clinica_id"
                                 baseTableName="profissional"
                                 constraintName="fk_profissional_clinica_id"
                                 referencedColumnNames="id"
                                 referencedTableName="clinica"/>

    </changeSet>
</databaseChangeLog>
